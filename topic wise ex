ONLY IF
#include<stdio.h>
int main()
{
    float a,b;
    printf("enter your age:");
    scanf("%f", &a);
     printf("enter your age:");
    scanf("%f", &b);

if((a>=12)&&(b<=19)){
    printf("you are eligible for voting");
}
return 0;
}

OUTPUT
enter your age:5.9
enter your age:63

