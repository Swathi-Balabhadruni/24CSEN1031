#include <stdio.h>

int main() {
  int num,q,r;
    printf("ENTER A NUMBER");
scanf("%d", &num);
q=num;
if(num>99 && num<=99999){
r= q%10;
printf("%d\n",r);
q=q/10;
r=q%10;
printf("%d\n",r);
q=q/10;
r=q%10;
printf("%d\n",r);
q=q/10;
    return 0;
}
}







#include <stdio.h>

int main() {
  int num,q,r,i;
    printf("ENTER A NUMBER");
scanf("%d", &num);
q=num;
while(q>0){
  r= q%10;
  printf("%d\n",r);
  q=q/10;
}  
return 0;
}
