#include<stdio.h>
int main() {
    int num;
    printf("enter an integer");
    scanf("%d", &num);
    int q;
    q = num/10;
    printf("%d\n",q);
    int r = num%10;
    printf("%d\n",r);
    r = q%10;
     printf("%d\n",r);
     q= q/10;
     r = q%10;
     printf("%d\n",r);
     q=q/10;
     printf("%d\n",q);
   return 0;  
}
